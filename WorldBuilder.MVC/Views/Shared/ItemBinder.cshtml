@model WorldBuilder.Data.PCtoItemBinding

@using (Html.BeginForm("PCtoItemBindings", "Create"))
{


    <div class="form-horizontal">
        <h4>PCtoItemBinding</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        
        @Html.Hidden("PCID", (object)ViewBag.PCID)
        <div class="col-md-10">
            @Html.DropDownList("ItemID", null, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}
